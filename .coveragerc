[run]
# Coverage configuration
source = app
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    */migrations/*
    */config.py

[report]
# Report options
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
