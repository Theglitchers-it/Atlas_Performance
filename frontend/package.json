{
  "name": "atlas-frontend",
  "version": "1.0.0",
  "description": "Frontend Vue.js per Piattaforma SaaS Personal Trainer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:mobile": "cross-env VITE_BUILD_TARGET=mobile vite build && npx cap sync",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "vue-tsc --noEmit",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android --livereload --external"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.8",
    "@capacitor/app": "^5.0.8",
    "@capacitor/camera": "^5.0.10",
    "@capacitor/core": "^5.6.0",
    "@capacitor/filesystem": "^5.2.2",
    "@capacitor/haptics": "^5.0.8",
    "@capacitor/keyboard": "^5.0.9",
    "@capacitor/network": "^5.0.8",
    "@capacitor/preferences": "^5.0.8",
    "@capacitor/push-notifications": "^5.0.10",
    "@capacitor/splash-screen": "^5.0.8",
    "@capacitor/status-bar": "^8.0.1",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/timegrid": "^6.1.10",
    "@fullcalendar/vue3": "^6.1.10",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.1.1",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.1",
    "date-fns": "^3.0.6",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.7.2",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.1.0",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^4.1.1",
    "eslint-plugin-vue": "^4.7.1",
    "happy-dom": "^20.6.1",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
